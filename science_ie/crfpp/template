# Unigram
U00:%x[-3,0]
U01:%x[-2,0]
U02:%x[-1,0]
U03:%x[0,0]
U04:%x[1,0]
U05:%x[2,0]
U06:%x[3,0]

#Bigrams
U07:%x[-1,0]/%x[0,0]
U08:%x[0,0]/%x[1,0]
U09:%x[1,0]/%x[2,0]
U0A:%x[-2,0]/%x[-1,0]

# last Char of prev and next
U10:%x[1,1]
U11:%x[-1,1]

# first 1-4
U20:%x[0,1]
U21:%x[0,2]
U22:%x[0,3]
U23:%x[0,4]

# last 1-4
U30:%x[0,5]
U31:%x[0,6]
U32:%x[0,7]
U33:%x[0,8]

# Character from neighbours
UA0:%x[-1,5]
UA1:%x[1,1]

# Lowercased
U40:%x[-2,9]
U41:%x[-1,9]
U42:%x[0,9]
U43:%x[1,9]
U44:%x[2,9]

#Bigrams
U47:%x[-1,9]/%x[0,9]
U48:%x[0,9]/%x[1,9]
U49:%x[1,9]/%x[2,9]
U4A:%x[-2,9]/%x[-1,9]


# Capitalization
U50:%x[0,10]
U51:%x[1,10]
U52:%x[-1,10]
U53:%x[0,10]/%x[1,10]
U54:%x[-1,10]/%x[0,10]

# Tags
U60:%x[0,11]
U61:%x[-1,11]
U62:%x[1,11]
U63:%x[0,11]/%x[1,11]
U64:%x[-1,11]/%x[0,11]
U65:%x[1,11]/%x[2,11]
U66:%x[-2,11]/%x[-1,11]

# Dict info
U70:%x[0,12]
U71:%x[0,13]
U72:%x[0,14]
U73:%x[0,15]
U74:%x[0,16]
U75:%x[0,17]

#Ref
U77:%x[0,18]
#U78:%x[0,19]
#U79:%x[0,20]

# Output
B0

